{
	"manifest_version": 2,
	"name": "Web Tools",
	"version": "1.0",
	"icons": {
		"16": "icon/16.png",
		"128": "icon/128.png"
	},
	"browser_action": {
		"default_title": "Web Tools",
		"default_icon": "icon/128.png"
	},
	"content_scripts": [{
		"css": ["content/content.css"],
		"js": ["lib/leaf.js", "lib/jquery.min.js", "content/content.js"],
		"all_frames": false,
		"matches": ["http://*/*", "https://*/*"],
		"run_at": "document_end"
	}],
	"chrome_url_overrides": {
		"newtab": "newtab/newtab.html"
	},
	"background": {
		"page": "background/background.html"
	},
	"key": "MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQC+vUHL61uBTiwO51y9xc5+AU0qGnDDDTa3xiuVkOLmEivnIvx1j1nZ38NOF3H5dSgc7JZrvw9xq/hvct4WlzRzMDXABOcrKbigqhBfoPvufWyAPSX4Zz8s5PTOnDCxuIm5yW65LH6igjMj2zOi9K4/QT6MoZJmalvdOPLFGQvLwQIDAQAB",
	"web_accessible_resources": ["*"],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": ["*://*/*", "cookies", "tabs", "contextMenus", "webRequestBlocking", "webRequest"]
}